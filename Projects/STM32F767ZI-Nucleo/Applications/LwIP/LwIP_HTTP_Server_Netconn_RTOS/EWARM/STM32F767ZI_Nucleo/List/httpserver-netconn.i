* @param  conn pointer on connection structure 
* @retval None
*/
void DynWebPage(struct netconn *conn)
{
  char PAGE_BODY[512];
  char pagehits[10] = {0};
  
  memset(PAGE_BODY, 0,512);
  
  /* Update the hit count */
  nPageHits++;
  sprintf(pagehits, "%d", (int)nPageHits);
